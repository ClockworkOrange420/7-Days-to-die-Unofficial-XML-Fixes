			<cell_rule name="default" prob="1"/>
		</ruleset>
		<ruleset name="vanillaMedium" world_size="8192" terrain_generator="vanilla">
			<cell_rule name="default" prob="1"/>
		</ruleset>
		<ruleset name="vanillaLarge" world_size="16384" terrain_generator="vanilla">
